{
  "name": "berelentless",
  "version": "1.0.0",
  "description": "A Fitness Goals App built with the MERN stack",
  "engines": {
    "node": "9.2.0"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mannycov/berelentless.git"
  },
  "author": "Emanuel Covarrubias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mannycov/berelentless/issues"
  },
  "homepage": "http://www.berelentless.io",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.6.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
